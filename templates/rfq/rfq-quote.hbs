{{{rfqQuoteMessage message}}}
{{#> rfq-card message=message}}
  {{#*inline "overrideBody"}}
      {{#switch message.state.state}}
          {{#case "rfq_acknowledged" "rfq_initiated" }}
             {{> rfq-start-ack message=message.product}}
          {{/case}}

          {{#case "rfq_priced" }}
             {{> rfq-priced message=message}}
          {{/case}}

          {{#case "rfq_agreed_pay" }}
             {{> rfq-agreed-pay message=message}}
          {{/case}}

          {{#case "rfq_confirmed" }}
             {{> rfq-confirmed message=message}}
          {{/case}}

          {{#case "rfq_passed"}}
             {{> rfq-passed message=message}}
          {{/case}}


      {{/switch}}
  {{/inline}}
{{/rfq-card}}
