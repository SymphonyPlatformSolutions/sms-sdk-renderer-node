<div>
  <card data-accent-color="tempo-bg-color--gray"
    data-icon-src="https://symphony.com/wp-content/uploads/2019/08/favicon.png">
    <header>
      {{#> overrideHeader}}
        {{#if message.title}}
        <span class="tempo-text-color--secondary">{{{message.title}}}</span>
        {{/if}}

        <ul>
          {{#each message.content}}
          <li>
            {{#if this.header}}
            <b>{{this.header}}</b>{{this.body}}
            {{else}}
            {{{this}}}
            {{/if}}
          </li>
          {{/each}}
        </ul>
      {{/overrideHeader}}
    </header>
  </card>
</div>
