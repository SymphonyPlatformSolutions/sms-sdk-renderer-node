<div>
  <card data-accent-color="tempo-bg-color--red"
    data-icon-src="https://symphony.com/wp-content/uploads/2019/08/favicon.png">
      <header>
        {{#> overrideHeader}}
          {{#if message.title}}
            <b>{{message.title}}</b>
          {{/if}}
        {{/overrideHeader}}
      </header>

      <body>
        {{#> overrideBody}}
          {{#if message.content}}
              {{message.content}}
          {{/if}}
        {{/overrideBody}}
      </body>
  </card>
</div>
